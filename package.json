  "type": "module",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --ignore data/ --ignore db-dev/ src/server.js",
    "debug": "nodemon --inspect=0.0.0.0:9229 --ignore data/ --ignore db-dev/ --trace-uncaught src/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:one": "node --experimental-vm-modules node_modules/jest/bin/jest.js -t",
    "lint": "eslint . --ext .js",
    "pretty": "prettier --write \"./**/*.{js,js,json,vue}\"",
    "generate-model": "sequelize-auto -o \"./models\" -d morphobank -h mb4-db-dev -u morphobank -p 3306 -x {password} -e mysql -l esm --cf k --cp o --cm p --sg",
    "generate-model-ts": "sequelize-auto -o \"./models\" -d morphobank -h mb4-db-dev -u morphobank -p 3306 -x {password} -e mysql -l ts --cf k --cp o --cm p --sg"
  },
  "keywords": [],
