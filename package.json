    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "decompress": "^4.2.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
